{
  "name": "mawk",
  "version": "0.1.0",
  "description": "Dependency injection and mocking for Node.js",
  "main": "index.js",
  "keywords": [
    "dependency",
    "inject",
    "mock"
  ],
  "author": "Christopher M. Baker",
  "license": "MIT",
  "scripts": {
    "test": "xo && istanbul cover node_modules/.bin/_mocha",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakerface/mawkjs.git"
  },
  "bugs": {
    "url": "https://github.com/bakerface/mawkjs/issues"
  },
  "homepage": "https://github.com/bakerface/mawkjs#readme",
  "xo": {
    "space": 2,
    "semicolon": true,
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ],
    "rules": {
      "brace-style": [
        2,
        "stroustrup"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ]
    }
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.3.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "should": "^8.2.2",
    "xo": "^0.13.0"
  }
}
